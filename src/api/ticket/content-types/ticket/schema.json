{
  "kind": "collectionType",
  "collectionName": "tickets",
  "info": {
    "singularName": "ticket",
    "pluralName": "tickets",
    "displayName": "ticket"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "tickets"
    },
    "event": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::event.event",
      "inversedBy": "tickets"
    },
    "quantity": {
      "type": "biginteger",
      "default": "1"
    },
    "paymentId": {
      "type": "text"
    },
    "ticket_type": {
      "type": "string"
    },
    "gen_price": {
      "type": "biginteger"
    },
    "vip_price": {
      "type": "biginteger"
    },
    "std_price": {
      "type": "biginteger"
    }
  }
}
