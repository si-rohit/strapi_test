{
  "kind": "collectionType",
  "collectionName": "tickets",
  "info": {
    "singularName": "ticket",
    "pluralName": "tickets",
    "displayName": "ticket"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "tickets"
    },
    "event": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::event.event",
      "inversedBy": "tickets"
    },
    "quantity": {
      "type": "biginteger",
      "default": "1"
    },
    "paymentMethod": {
      "type": "enumeration",
      "enum": [
        "stripe",
        "razorpay"
      ]
    },
    "paymentStatus": {
      "type": "enumeration",
      "enum": [
        "pending",
        "success",
        "failed"
      ]
    },
    "paymentId": {
      "type": "text"
    }
  }
}
