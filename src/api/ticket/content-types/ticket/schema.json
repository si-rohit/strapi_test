      "target": "api::event.event",
      "inversedBy": "tickets"
    },
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "price": {
      "type": "biginteger"
    },
    "totalQuantity": {
      "type": "biginteger"
    },
    "soldQuantity": {
      "type": "biginteger"
    },
    "order_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order-item.order-item",
      "mappedBy": "ticket"
    },
    "user_tickets": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-ticket.user-ticket",
      "mappedBy": "ticket"
    }
  }
